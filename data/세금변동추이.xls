	function fnSetDisplayTerm() {
		var term = $("input[name='TERM']:checked").val();
		
		if(term == "T") {
			$("#time_area").attr('style', 'display:');
			$("#time_area1").attr('style', 'display:');
			$("#time_area2").attr('style', 'display:');
			$("#time_area3").attr('style', 'display:');
			$("#time_area4").attr('style', 'display:');
			$("#time_area5").attr('style', 'display:');
			$("#product_area").attr('style', 'display:none');
			$("#product_area1").attr('style', 'display:none');
			$("#product_area2").attr('style', 'display:none');
			$("#product_area3").attr('style', 'display:none');
			$("#product_area4").attr('style', 'display:none');
			$("#product_area5").attr('style', 'display:none');
		}
		
		else if(term == "C") {
			$("#time_area").attr('style', 'display:none');
			$("#time_area1").attr('style', 'display:none');
			$("#time_area2").attr('style', 'display:none');
			$("#time_area3").attr('style', 'display:none');
			$("#time_area4").attr('style', 'display:none');
			$("#time_area5").attr('style', 'display:none');
			$("#product_area").attr('style', 'display:');
			$("#product_area1").attr('style', 'display:');
			$("#product_area2").attr('style', 'display:');
			$("#product_area3").attr('style', 'display:');
			$("#product_area4").attr('style', 'display:');
			$("#product_area5").attr('style', 'display:');
		}
	}

	$().ready(function(){
	
		// 라디오 버튼 선택시 클릭 이벤트 설정
		$("input[name='TERM']").change(function() {
			// 기간영역 활성화/ 비활성화
			fnSetDisplayTerm();
		});
		
		fnSetDisplayTerm();
  });
</script>
